label live-@FLAVOUR@
	menu label ^Live Resuce Environment
	menu default
	linux @LINUX@
	initrd @INITRD@
	append @APPEND_LIVE@

label live-@FLAVOUR@-failsafe
	menu label ^Live Rescue Environment (nomodeset)
	linux @LINUX@
	initrd @INITRD@
	append @APPEND_LIVE_FAILSAFE@

label memtest
	menu label ^Memtest 86+
	linux /live/memtest
