label live-@FLAVOUR@
	menu label ^Boot to the diagnostics and recovery environment
	menu default
	linux @LINUX@
	initrd @INITRD@
	append @APPEND_LIVE@

label live-@FLAVOUR@-failsafe
	menu label ^Boot to the environment in graphics failsafe mode (nomodeset)
	linux @LINUX@
	initrd @INITRD@
	append @APPEND_LIVE_FAILSAFE@